{
  "name": "KanHub",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "omfgitsklampz",
  "description": "blueee",
  "homepage_url": "https://github.com/UTSCC09/omfgitsklampz",
  "browser_action": {
      "default_title": "KanHub",
      "default_popup": "index.html"
  },
  "web_accessible_resources": [
      "localhost:8080/*",
      "settings.html"
  ],
  "permissions": [
    "tabs",
    "https://github.com/*",
    "https://api.github.com/*",
    "cookies",
    "http://localhost:8080/",
    "https://localhost:8080/"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["https://github.com/*"],
      "js": [
        "static/js/content.js"
      ]
    }
  ],
  "background": {
    "scripts": ["backgroundpage.js"]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
  
//   "optional_permissions": [
//     "<all_urls>"
